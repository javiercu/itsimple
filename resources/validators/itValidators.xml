<?xml version="1.0" encoding="ISO-8859-1"?>
<itValidators>
	<toolInformation>
		<name>itSIMPLE</name>
		<version>3.5.10</version>
	</toolInformation>
	<settings>
		<timeout enabled="false">1200</timeout>
		<planValidation enabled="true" />
	</settings>
	<validators>
		<validator id="1">
			<name>VAL</name>
			<version>version 4.2.07 Linux</version>
			<date>July 2010</date>
			<institution />
			<author>R. Howey, D. Long, and  M. Fox</author>
			<link>http://planning.cis.strath.ac.uk/VAL/</link>
			<description />
			<platform>
				<linux />
			</platform>
			<requirements>
				<pddl21 />
				<pddl22 />
				<pddl30 />
			</requirements>
			<settings>
				<filePath>myValidators/val4</filePath>
				<output hasOutputFile="false">
					<outputFile>
						<fileName></fileName>
						<fileNameAutomaticIncrement />
						<argument needArgument="false" parameter="">
							<description />
						</argument>
						<additionalGeneratedFiles>
							<fileName></fileName>
						</additionalGeneratedFiles>
					</outputFile>
					<consoleOutput>
						<planStartIdentifier startsAfterNlines="" />
						<planEndIdentifier />
					</consoleOutput>
				</output>
				<arguments>
					<argument parameter="-f" id="4">
						<name>enforced hill-climbing</name>
						<value>report</value>
						<enable>false</enable>
						<description>-f file -- LaTeX report will be stored in file 'file.tex'</description>
					</argument>
					<domain parameter="" id="1">
						<name>domain</name>
						<value />
						<enable>true</enable>
						<description>specifies the file of the operators</description>
					</domain>
					<problem parameter="" id="2">
						<name>problem</name>
						<value />
						<enable>true</enable>
						<description>specifies the file of (init/goal) facts</description>
					</problem>
					<plan parameter="" id="3">
						<name>problem</name>
						<value />
						<enable>true</enable>
						<description>specifies the file of the plan</description>
					</plan>
				</arguments>
				<timeout enabled="false">36000</timeout>
			</settings>
		</validator>
	</validators>
</itValidators>

